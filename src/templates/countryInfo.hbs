{{#each this}}
<h2>{{name}}</h2>
<div class="wrapper">
  <div>
  <p>Capital: {{capital}}</p>
  <p>Population: {{population}}</p>
  <p>Languages:</p>
  <ul>
    {{#each languages}}
    <li>{{this.name}}</li>
    {{/each}}
  </ul>
</div>
<div>
  <img src="{{flag}}" alt="flag" width="400">
</div>
</div>
{{/each}}